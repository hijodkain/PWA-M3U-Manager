{
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/youtube_live_monitor",
      "destination": "/api/youtube_live_monitor"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.1.1"
    }
  },
  "installCommand": "python3 -m pip install --upgrade pip && python3 -m pip install -r requirements.txt"
}